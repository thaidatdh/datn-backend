                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "default": true
                    },
                    "practice": {
                      "$ref": "#/components/schemas/Practice"
                    }
                  }
                }
              }
            }
          }
        }
