{
  "openapi": "3.0.2",
  "info": {
    "description": "ADC RESTful API.",
    "version": "1.0.0",
    "title": "ADC RESTful API."
  },
  "servers": [{ "url": "http://localhost:3000/api" }],
  "components": {
    "schemas": {
      "Practice": {
        "type": "object",
        "properties": {
