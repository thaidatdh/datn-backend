        "tags": ["Practice"],
        "summary": "Delete practice",
        "description": "Delete practice",
        "parameters": [
          {
            "name": "practice_id",
            "in": "path",
            "description": "Practice ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "default": true
                    },
                    "message": {
                      "type": "string",
                      "default": "Delete Successfully"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
