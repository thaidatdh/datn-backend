        "summary": "Get practice detail",
        "description": "Get practice detail",
        "parameters": [
          {
            "name": "practice_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "default": true
                    },
                    "practice": {
                      "$ref": "#/components/schemas/Practice"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["Practice"],
        "summary": "Update practice",
        "description": "Update practice",
        "parameters": [
          {
            "name": "practice_id",
            "in": "path",
            "description": "Practice ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "address": {
                    "type": "string"
                  },
                  "phone": {
                    "type": "string"
                  },
                  "fax": {
                    "type": "string"
                  },
                  "default_provider": {
                    "type": "string",
                    "example": "60488f2d8d74df3204f559d8",
